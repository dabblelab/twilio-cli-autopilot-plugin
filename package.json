    "@oclif/plugin-help": "^2.2.3",
    "@twilio-labs/serverless-api": "^4.0.1",
    "@twilio/cli-core": "^7.0.0",
    "csv-parse": "^4.8.2",
    "extract-zip": "^1.6.7",
    "ora": "^3.4.0",
    "pkg-install": "^1.0.0",
    "pretty-json-stringify": "0.0.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "simple-git": "^3.5.0",
    "twilio-run": "^2.8.0",
    "underscore": "^1.12.1",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.7",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.1",
    "husky": "^4.3.0",
    "mocha": "^8.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/oclif.manifest.json",
    "/src",
    "README.md"
  ],
  "homepage": "https://github.com/tingiris/twilio-cli-autopilot-plugin",
  "keywords": [
    "oclif-plugin",
    "twilio-cli-plugin",
    "twilio-ci",
    "twilio",
    "autopilot",
    "autopilot-cli"
